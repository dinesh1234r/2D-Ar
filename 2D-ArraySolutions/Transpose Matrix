class Solution {
    public int[][] transpose(int[][] matrix) {
        int len=matrix.length;
        int len1=matrix[0].length;
        int[][] mat1=new int[len1][len];
        for(int i=0;i<len;i++)
        {
            for(int j=0;j<len1;j++)
            {
                mat1[j][i]=matrix[i][j];
            }
        }
        return mat1;
    }
}
